{
  "ConnectionStrings": {
    "SurveyItechArt": "Server=(LocalDb)\\MSSQLLocalDB;AttachDBFilename=|DataDirectory|\\SurveyItechArt.mdf;Trusted_Connection=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs\\Survey.log",
          "outputTemplate": "{Timestamp} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}